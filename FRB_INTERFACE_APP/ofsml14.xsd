			</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="ofsFaultCode" type="ofsml14FaultCode"/>
            <xsd:element name="ofsFaultMessage" nillable="true" type="ofsml14FaultMessage"/>
            <xsd:element minOccurs="0" name="ofsFaultContext" type="xsd:anyURI"/>
            <xsd:element minOccurs="0" name="ofsFaultDetails" type="ofsml14FaultDetails"/>
        </xsd:sequence>
    </xsd:group>
    <!-- OFSML SUPER TYPES DEFINITIONS -->
